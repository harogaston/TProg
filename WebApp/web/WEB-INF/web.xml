<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<servlet>
		<servlet-name>IniciarSesion</servlet-name>
		<servlet-class>tprog.web.IniciarSesion</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Home</servlet-name>
		<servlet-class>tprog.web.Home</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EstadoSesion</servlet-name>
		<servlet-class>tprog.web.EstadoSesion</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CerrarSesion</servlet-name>
		<servlet-class>tprog.web.CerrarSesion</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CtrlCarga</servlet-name>
		<servlet-class>tprog.web.carga.CtrlCarga</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Home</servlet-name>
		<url-pattern>/Home/*</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>IniciarSesion</servlet-name>
		<url-pattern>/IniciarSesion</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CerrarSesion</servlet-name>
		<url-pattern>/CerrarSesion</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CtrlCarga</servlet-name>
		<url-pattern>/CargarDatos</url-pattern>
	</servlet-mapping>
	<session-config>
		<session-timeout>
			30
		</session-timeout>
	</session-config>
</web-app>
